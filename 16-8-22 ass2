#include<stdio.h>
void main()
{
	int a[5],i;
	int j,flag;
	printf("enter elements into array: ");
	for(i=0;i<5;i++)
		scanf("%d",&a[i]);
	printf("The prime numbers of array: ");
	for(i=0;i<5;i++)
	{
		flag=0;
		if(a[i]!=1)
		{
			for(j=2;j<a[i];j++)
			{
				if(a[i]%j==0)
				{
					flag=1;
					break;
				}
			}
			if(flag==0)
			printf("%d\t",a[i]);
		}
	}
	
}
