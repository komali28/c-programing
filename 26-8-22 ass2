include<stdio.h>
int main()
{
	char str[40];
	gets(str);
	int i,count1=0,count2=0,count3=0;
	for(i=0;str[i]!='\0';i++)
	{
		if(isupper(str[i]))
			count1=count1+1;
		}
		printf("%d",count1);
	}
}
	if(islower(str[i]))
	{
	count2=count2+1;
	printf("%d"count2);
	}
	if(isdigit(str[i]))
	{
		count3=count3+1;
		printf("%d",count3);
	}
